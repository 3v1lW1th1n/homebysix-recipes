<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the latest version of SpiderOakONE and builds a package.</string>
    <key>Identifier</key>
    <string>com.github.homebysix.pkg.SpiderOakONE</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>SpiderOakONE</string>
        <key>BUNDLE_ID</key>
        <string>com.spideroak.orange.client</string>
        <key>pkg_path</key>
        <string>%pathname%</string>
    </dict>
    <key>MinimumVersion</key>
    <string>0.5.0</string>
    <key>ParentRecipe</key>
    <string>com.github.homebysix.download.SpiderOakONE</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>Copier</string>
            <key>Arguments</key>
            <dict>
                <key>source_path</key>
                <string>%pathname%/%NAME%.pkg</string>
                <key>destination_path</key>
                <string>%RECIPE_CACHE_DIR%/%NAME%.pkg</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>
